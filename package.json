{
  "name": "proxy-request",
  "version": "0.1.0",
  "engines": {
    "node": ">=6.1.0"
  },
  "description": "a proxy tool by request which damn convenient with stream pipe",
  "main": "lib/index.js",
  "files": [
    "lib",
    "README.md"
  ],
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "request": "^2.74.0"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "istanbul": "^0.4.3",
    "mocha": "^2.4.5",
    "nodemon": "^1.9.2"
  },
  "scripts": {
    "prepublish": "npm run mocha && npm run build",
    "build": "node_modules/.bin/babel lib/index.js -o dist/proxy-request.js",
    "test": "node_modules/.bin/nodemon --exec npm run mocha",
    "mocha": "node_modules/.bin/istanbul cover node_modules/.bin/_mocha",
    "debug-test": "npm run mocha -- debug"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wenshin/proxy-request.git"
  },
  "keywords": [
    "promise",
    "all"
  ],
